{
  "name": "securevault-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "prisma": {
    "seed": "ts-node src/seed/seed.ts"
  },
  "scripts": {
    "dev": "nodemon -L src/index.ts",
    "start:dev": "cross-env ENVIRONMENT=development nodemon -L src/index.ts",
    "start:prod": "cross-env ENVIRONMENT=production ts-node src/index.ts",
    "build": "nest build",
    "deploy:dev": "pm2 startOrRestart ecosystem.config.js --env development --update-env --log-date-format 'DD-MM HH:mm:ss.SSS'",
    "deploy:prod": "pm2 startOrRestart ecosystem.config.js --env production --update-env --log-date-format 'DD-MM HH:mm:ss.SSS'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings 0",
    "pre-migrate:dev": "dotenv -e .env.development -- npx prisma db pull && npx prisma generate",
    "pre-migrate:prod": "dotenv -e .env.development -- npx prisma db pull && npx prisma generate",
    "migrate:prod": "dotenv -e .env.production -- npx prisma db push",
    "migrate:dev-server": "dotenv -e .env.development -- npx prisma db push",
    "push:prod": "dotenv -e .env.production -- npx prisma db push",
    "push:dev": "dotenv -e .env -- npx prisma db push",
    "pull:prod": "dotenv -e .env.production -- npx prisma db pull",
    "pull:dev": "dotenv -e .env -- npx prisma db pull",
    "seed:prod": "dotenv -e .env.production -- ts-node prisma/seed.ts",
    "seed:dev": "dotenv -e .env -- ts-node prisma/seed.ts",
    "query": "ts-node src/query.ts"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "eslint --cache --fix"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "cors": "^2.8.5",
    "envalid": "^7.3.1",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@prisma/client": "^5.1.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "serve-favicon": "^2.5.0"
  }
}
